 #include <stdio.h> 
int main ()
  {
  
   int m;
   printf("Enter marks: ");
   scanf("%d",&m);
   
   if(m<0 || m>100)
   printf("Wrong entry");
   
   if(m>85)
   printf("Grade A");
   
   if(m>=70&& m<85)
   printf("Grade B");
   
   if(m>=55 && m<70)
   printf("Grade C");
   
    if(m>=40&& m<55)
   printf("Grade D");
   
   if(m<40)
   printf("Grade F");
   
   return 0;
  }
